{
    "version": 2,
    "name": "client",
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "app.js",
        "headers": { "cache-control": "s-maxage=0" },
        "dest": "/index.html"
      }
    ]
  }