{
    "version": 2,
    "name": "client",
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "app.js",
        "headers": { "cache-control": "s-maxage=0" },
        "dest": "/index.html"
      },
      {
        "src": "components/profile/profile.js",
        "headers": { "cache-control": "s-maxage=0" },
        "dest": "/profile.html"
      }
    ],
    "build": {
      "env": {
        "REACT_APP_CLIENT_ID": "f4fmRQmmf6YrrsMPmWDyVD3SAWB9258r",
        "REACT_APP_DOMAIN_URL": "focus-timer.auth0.com"
      }
    }
  }